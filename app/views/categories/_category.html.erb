<div id="<%= dom_id category %>">
  <div class="d-flex">
      <%= image_tag "#{category.icon}", size: "150x150", alt: 'icon' %>
    <div>
      <h3><%= category.name %></h3>
      <span class="fs-6 fw-light"><%= category.created_at %></span>

      <%#= Transaction.total_amount(@category.transaction) %>
    </div>
  </div>
</div>
